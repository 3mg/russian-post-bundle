{
    "name" : "3mg/russian-post-bundle",
    "description" : "Api для работы с сервисом отслеживания Почты России",
    "keywords" : [
        "russian post"
    ],
    "homepage" : "https://github.com/3mg/russian-post-bundle",
    "license" : "MIT",
    "authors" : [{
            "name" : "Nikolay Savchenko",
            "email" : "3mg@bk.ru"
        }
    ],
    "require": {
        "symfony/intl": "^2.7",
        "jms/serializer": "~0.11",
        "fergusean/nusoap": "^0.9.5",
        "goetas/xsd-reader":"2.*@dev"
    },
    "suggest": {
        "jms/serializer-bundle": "For using as symfony bundle"
    },
    "repositories": [
        {
          "type": "git",
          "url": "https://github.com/3mg/nusoap"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "a3mg\\RussianPostBundle" : "/"
        }
    },
    "scripts": {
        "build": "xsd2php convert:php 'Resources/xml/russian_post.xsd' --ns-map='http://russianpost.org/operationhistory/data;a3mg/RussianPostBundle/Model/' --ns-dest='a3mg/RussianPostBundle/Model/;Model' --ns-map='http://schemas.xmlsoap.org/soap/envelope/;a3mg/RussianPostBundle/Model/'",
        "build_metadata": "xsd2php convert:jms-yaml 'Resources/xml/russian_post.xsd' --ns-map='http://russianpost.org/operationhistory/dataa3mg/RussianPostBundle/Model/' --ns-dest='a3mg/RussianPostBundle/Model/;Resources/config/serializer' --ns-map='http://schemas.xmlsoap.org/soap/envelope/;a3mg/RussianPostBundle/Model/'"
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.1-dev"
        }
    }
}
